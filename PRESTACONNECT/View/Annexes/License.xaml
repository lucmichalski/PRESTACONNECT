<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="PRESTACONNECT.License"
	x:Name="Window"
    xmlns:dg="clr-namespace:Microsoft.Windows.Controls;assembly=WPFToolkit"
	Title="Gestion de la licence"
	Width="480" Height="156"
	WindowStyle="ThreeDBorderWindow" 
    Icon="/PRESTACONNECT;component/prestaconnect.ico" 
    ResizeMode="NoResize" 
    WindowStartupLocation="CenterScreen"
	ShowInTaskbar="True"
    Background="{DynamicResource ResourceKey=WindowBackgroundBrush}" >
    <Grid>
        <Image Source="/PRESTACONNECT;component/prestaconnect.ico" Margin="12,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="64" />
        <TextBlock Name="TextBlockLicense" Margin="86,0,12,0" TextAlignment="Justify" Height="Auto" VerticalAlignment="Center" TextWrapping="Wrap" FontSize="14"/>
    </Grid>
</Window>